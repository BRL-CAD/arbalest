$Color-StatusBar                        : #0a5466;
$Color-StatusBarText                    : #ffffff;
$Color-ObjectTreeWidgetItemText         : #000000;
$Color-ToolbarSeparator                 : #2f2f2f;
$Color-HelpWidget                       : #737373;
$Color-GraphicsView                     : #797979;
$Color-ColorText                        : #99beff;  // for colorful text
$Color-IconFile                         : #6c0ad0;
$Color-IconView                         : #25a2c1;
$Color-IconRaytrace                     : #d61d32;
$Color-Cell                             : #606060;
$Color-WindowTitleButtons               : #c0ffa2;

$Color-FullyVisibleObjectText           : #1491e9;  // object tree item text.
$Color-SomeChildrenVisibleObjectText    : #26d7c0;  // object tree item text.
